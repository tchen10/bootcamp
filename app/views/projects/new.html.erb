<h1>New Project</h1>


<%= form_tag(projects_url, method: 'post') do %>


  <div>
    <%= label_tag :title %><br />
    <%= text_field_tag :title %>
  </div>

    <div>
    <%= label_tag :description %><br />
    <%= text_field_tag :description %>
  </div>

    <div>
    <%= label_tag :date_created %><br />
    <%= date_select_tag :date_created %>
  </div>

    <div>
    <%= label_tag :date_completed %><br />
    <%= date_select_tag :date_completed %>
  </div>

    <div>
    <%= label_tag :due_date %><br />
    <%= date_select_tag :due_date %>
  </div>

    <div>
    <%= label_tag :status %><br />
    <%= text_field_tag :status %>
  </div>

  
  <div>
    <%= submit_tag "Create Project" %>
  </div>
<% end %>
